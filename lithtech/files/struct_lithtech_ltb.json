      "0": "cmp_none",
      "1": "cmp_relevant",
      "2": "cmp_relevant_16",
      "3": "cmp_relevant_rot16"
    },
    "obb_version": {
      "23": "v23",
      "24": "v24",
      "25": "v25"
    },
    "error_mark": {
      "255": "invalid_bone"
    }
  },
  "seq": [
    {
      "id": "header",
      "type": "header"
    },
    {
      "id": "body",
      "type": "body"
