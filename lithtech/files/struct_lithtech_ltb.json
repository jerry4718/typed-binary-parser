          "type": "s4"
        },
        {
          "id": "num_pieces_no_use",
          "type": "s4"
        },
        {
          "id": "num_child_models",
          "type": "s4"
        },
        {
          "id": "num_faces",
          "type": "s4"
        },
        {
          "id": "num_vertexes",
          "type": "s4"
        },
        {
          "id": "num_vertex_weights",
          "type": "s4"
        },
        {
          "id": "num_lod",
          "type": "s4"
        },
        {
          "id": "num_sockets",
